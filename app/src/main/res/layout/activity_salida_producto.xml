<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SalidaProducto">

    <ImageView
            android:id="@+id/imageView9"
            android:layout_width="137dp"
            android:layout_height="109dp"
            app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.22"
            app:srcCompat="@mipmap/almacenamiento"/>

    <Button
            android:id="@+id/btnRetirar"
            android:layout_width="272dp"
            android:layout_height="45dp"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/button_rounded"
            android:text="Retirar"
            android:textColor="#ffff"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="56dp"
            app:layout_constraintTop_toBottomOf="@+id/spNombreSalida"
            app:layout_constraintVertical_bias="1.0"/>

    <Button
            android:id="@+id/btnRegistrarCodigoSalida"
            android:layout_width="117dp"
            android:layout_height="40dp"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/button_rounded"
            android:text="Registrar"
            android:textColor="#ffff"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36"
            />

    <Spinner
            android:id="@+id/spNombreSalida"
            android:layout_width="261dp"
            android:layout_height="51dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.689"
            app:layout_constraintHorizontal_bias="0.486"/>

    <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre"
            android:textAlignment="center"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/spNombreSalida"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.973"/>

    <EditText
            android:id="@+id/txtCodigoSalida"
            android:layout_width="259dp"
            android:layout_height="51dp"
            android:ems="10"
            android:hint="Codigo Nueva Salida"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.053"
            app:layout_constraintHorizontal_bias="0.48"/>

    <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CategorÃ­a"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/spCategoriaSalida"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.959"/>

    <Spinner
            android:id="@+id/spCategoriaSalida"
            android:layout_width="266dp"
            android:layout_height="48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.541"
            app:layout_constraintHorizontal_bias="0.468"/>

    <EditText
            android:layout_width="261dp"
            android:layout_height="55dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/txtCantidadSalida"
            app:layout_constraintTop_toBottomOf="@+id/spNombreSalida"
            app:layout_constraintBottom_toTopOf="@+id/btnRetirar"
            app:layout_constraintVertical_bias="0.519"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:hint="Cantidad"/>

</androidx.constraintlayout.widget.ConstraintLayout>