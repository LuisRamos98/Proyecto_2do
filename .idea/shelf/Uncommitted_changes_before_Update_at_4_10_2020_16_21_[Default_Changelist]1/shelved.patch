+public class AgregarUsuario extends AppCompatActivity
+        implements Response.Listener<JSONObject>, Response.ErrorListener {
+
+    //Variables de la activity
+    private EditText txtNombre, txtApellido, txtCedula, txtCorreo, txtUsuario, txtPass;
+    private Button btnRegistar;
+    //Variables para la conexion con el WebService
+    RequestQueue requestQueue;
+    JsonObjectRequest jsonObjectRequest;
 
     @Override
     protected void onCreate(Bundle savedInstanceState) {
         super.onCreate(savedInstanceState);
         setContentView(R.layout.activity_agregar_usuario);
+        //relacion Logica-Grafica
+        txtNombre = (EditText)findViewById(R.id.txtNombre);
+        txtApellido = (EditText)findViewById(R.id.txtApellido);
+        txtCedula = (EditText)findViewById(R.id.txtCedula);
+        txtCorreo = (EditText)findViewById(R.id.txtCorreo);
+        txtUsuario = (EditText)findViewById(R.id.txtUser);
+        txtPass = (EditText)findViewById(R.id.txtPass);
+        btnRegistar = (Button)findViewById(R.id.btnRegistrarUsuario);
+        //Instancia del objeto para la conexion remota
+        requestQueue = Volley.newRequestQueue(this);
+        //modificar el evento click
+        btnRegistar.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                RegistrarUsuario();
+            }
+        });
+    }
+
+    //Funcion para registrar usuarios
+    private void RegistrarUsuario() {
+        String nombre, apellido, ci, correo, usuario, pass;
+        nombre = txtNombre.getText().toString();
+        apellido = txtApellido.getText().toString();
+        ci = txtCedula.getText().toString();
+        correo = txtCorreo.getText().toString();
+        usuario = txtUsuario.getText().toString();
+        pass = txtPass.getText().toString();
+        if(!nombre.isEmpty() && !apellido.isEmpty() && !ci.isEmpty() && !correo.isEmpty() && !usuario.isEmpty() && !pass.isEmpty()){
+            //llamada al WebService
+            String url = "http://192.168.1.2/BDremota/wsJSONRegistrousuario.php?empresa="+MainActivity.idEmpresa
+                    +"&cedula="+ci+"&nombre="+nombre+"&apellido="+apellido+"&correo="+correo+"&user="
+                    +usuario+"&pass="+pass;
+            url = url.replace(" ","%20");
+            jsonObjectRequest = new JsonObjectRequest(Request.Method.GET, url, null, this, this);
+            requestQueue.add(jsonObjectRequest);
+        } else {
+            Toast.makeText(getApplicationContext(), "¡Campo Vacio!\nIntente nuevamente", Toast.LENGTH_SHORT).show();
+        }
+    }
+
+    //Funcion donde Se trata la respuesta del WebService
+    @Override
+    public void onResponse(JSONObject response) {
+        if(response.has("Usuario")){
+            ValidarRegistroUsuario(response);
+        }
+    }
+
+    //Funcion para validar si el usuario ya esta registrado o no
+    private void ValidarRegistroUsuario(JSONObject response) {
+        int id_usuario;
+        try {
+            JSONArray jsonArray = response.getJSONArray("Usuario");
+            JSONObject jsonObject = jsonArray.getJSONObject(0);
+            id_usuario = jsonObject.getInt("id");
+            if(id_usuario != 0){
+                Toast.makeText(getApplicationContext(), "Registro con Exito", Toast.LENGTH_SHORT).show();
+                LimpiarCampos();
+            } else{
+                Toast.makeText(getApplicationContext(), "Registro ya Existente en Usuario o CI\nintente Nuevamente", Toast.LENGTH_SHORT).show();
+                txtUsuario.setText("");
+                txtCedula.setText("");
+            }
+        } catch(JSONException e){
+            e.printStackTrace();
+            Toast.makeText(this, e.toString(), Toast.LENGTH_SHORT).show();
+        }
+    }
+
+    //Funcion para limpiar los campos de texto de las vistas
+    private void LimpiarCampos() {
+        txtNombre.setText("");
+        txtApellido.setText("");
+        txtCedula.setText("");
+        txtCorreo.setText("");
+        txtUsuario.setText("");
+        txtPass.setText("");
+    }
+
+    //Funcion que recepta el error en caso de existir cuando se llame al WS
+    @Override
+    public void onErrorResponse(VolleyError error) {
+        Toast.makeText (getApplicationContext(), "No se pudo registrar\nError: "+error.toString(), Toast.LENGTH_SHORT).show();
     }
 }
